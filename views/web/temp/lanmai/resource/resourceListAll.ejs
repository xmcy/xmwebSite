		var demandSummary = $(".demand_summary"),
				sorting = $(".addIcon").parent(),
				requireselect = $(".nav li"),
				budgetmin = $(".budgetmin"),
				budgetmax = $(".budgetmax"),
				deletecategory = $(".deleteIcon").parent(),
