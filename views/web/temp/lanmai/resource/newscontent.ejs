<style>
    .charonDigitalHeaderTopCon>ul>li>a{
        color: #908e8e;
    }
    .haveListForMoney{
        position: relative;
    }
    .haveListForMoney img {
        position: absolute;
        width: 20%;
        top: -4px;
        right: -2px;
    }
    .bgimg{
        margin-top: 10px;
        width: 100%;
        height: 340px;
        background: url("/themes/<%=staticforder%>/images/newsbg.png");
    }
    .bgimg>img{
        width: 1200px;
        height: 340px;
        display: block;
        margin: 0 auto;
    }
    #pagination {
        margin-left: 220px;
        margin-top:50px;
    }
    ul.pagination>li>a{
        margin-right: 6px;
    }
    .content{
        height: auto;
        width: 100%;
        margin:0 auto;
    }
    .aritleinfo{
        width: 100%;
        height: 130px;
        background-color: #f2f2f2;
    }
    .aritleinfo>div{
        text-align: center;
        font-size: 18px;
    }
    .aritleinfo>.title{
        font-size: 26px;
        line-height: 90px;
    }
    .articlecontent{
        width: 1200px;
        margin: 20px auto;
        height: auto;
        line-height: 28px;
        font-size: 16px;
    }
    .articlecontent img{
        width: 700px;
        height: auto;
        display: block;
        margin: 10px auto;
    }
</style>
<link href="/plugins/bootstrap/css/bootstrap.min.css?vesion=<%=siteConfig.version%>" rel="stylesheet">
<link href="/plugins/datetimePicker/dateSelect.css" rel="stylesheet">
<script src="/plugins/datetimePicker/dateSelect.js"></script>
<script src="/javascripts/angular.min.js?vesion=<%=siteConfig.version%>" type="text/javascript"></script>
<script src="/plugins/angular-ui-bootstrap/dist/ui-bootstrap-tpls-0.12.0.min.js?vesion=<%=siteConfig.version%>" type="text/javascript"></script>

<div ng-app="layoutNewslist" ng-controller="layoutforNewslist">
      <div class="bgimg"><img src="/themes/<%=staticforder%>/images/news.png"/></div>
    <div class="content">
          <div class="aritleinfo">
              <div class="title" title="<%=articleDetail.title%>"><%=articleDetail.title%></div>
              <div><%=moment(articleDetail.createdAt).format('YYYY-MM-DD hh:mm')%></div>
          </div>
        <div class="articlecontent">
            <!--<img src="/themes/<%=staticforder%>/images/bcpi.png"/>-->
            <input type="hidden" id="detail" value="<%=articleDetail.discription%>"/>
            <div id="contentDetail"></div>
        </div>
    </div>
</div>
<script>
    $(function () {
        $("#contentDetail").html($("#detail").val())
    })
</script>
<script src="/themes/<%=staticforder%>/js/modules.js?vesion=<%=siteConfig.version%>" type="text/javascript"></script>
<script src="/themes/<%=staticforder%>/js/layoutNewslistController.js?vesion=<%=siteConfig.version%>" type="text/javascript"></script>
