    </div>
</div>
<script>
    $(function(){
        $('.loadingNum>span>img').on("click",function(){
            $(this).attr("src","/themes/lanmai/images/check_on.png")
            $(".loadingNum001").attr("checked","checked")
            $(".loadingNum002").removeAttr("checked")
            $(".onInputPhoneNum,.onInputCodeNum").css({display:"none"})
            $(".onInputNum>span>img").attr("src","/themes/lanmai/images/check_off.png");
            $(".onInputPhoneNum>input").attr("disabled","disabled").css({border:"1px solid #C1C1C1"});
            $(".onInputCodeNum>input").attr("disabled","disabled").css({border:"1px solid #c1c1c1"});
        })
        $('.onInputNum>span>img').on("click",function(){
            $(this).attr("src","/themes/lanmai/images/check_on.png")
            $(".loadingNum002").attr("checked","checked")
            $(".loadingNum001").removeAttr("checked")
            $(".onInputPhoneNum,.onInputCodeNum").css({display:"block"})
            $(".loadingNum>span>img").attr("src","/themes/lanmai/images/check_off.png");
            $(".onInputPhoneNum>input").removeAttr("disabled").css({border:"1px solid #A9A9A9"});
            var rightNum = /^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9])\d{8}$/;
            var nowNum = $(".onInputPhoneNum>input").val();
            var MatchOk = nowNum.match(rightNum);
            if(MatchOk){
                $(".onInputPhoneNum>input").attr("disabled","disabled").css({border:"1px solid #c1c1c1"});
                $(".onInputCodeNum>input").removeAttr("disabled").css({border:"1px solid #777"});
                $(".fixClickSendNum").removeAttr("disabled").css({background:"#777"})
            }
        })
        //发布时输入新的手机号
        $(".onInputPhoneNum>input").keyup(function(){
            var rightNum = /^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9])\d{8}$/;
            var nowNum = $(".onInputPhoneNum>input").val();
            var MatchOk = nowNum.match(rightNum);
            if(MatchOk){
                $(".fixClickSendNum").removeAttr("disabled").css({background:"#777"})
            }else{
                $(".fixClickSendNum").attr("disabled","disabled")
            }
        })
        var rightNum = /^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9])\d{8}$/;
        $(".fixClickSendNum").click(function(){
            var nowNum = $(".onInputPhoneNum>input").val();
            var Matchok = nowNum.match(rightNum);

            if(!Matchok){
                $(".onInputPhoneNumTips").html("手机号格式不正确！").css({color:"#DA2F2F"})
                setTimeout(function () {
                    $(".onInputPhoneNumTips").html("");
                },3000)
                return
            }else{
                $(".onInputPhoneNumTips").html("手机号格式正确！").css({color:"#9EEA6A"});
                $(".onInputPhoneNum>input").attr("disabled","disabled").css({border:"1px solid #A9A9A9"});
                setTimeout(function () {
                    $(".onInputPhoneNumTips").html("");
                },3000);
            }
            var data={
                phoneNum:nowNum
            }
            $.ajax({
                type:"POST",
                url:"/users/sendVerifyCode",
                data:JSON.stringify(data),
                contentType: "application/json",
                success:function(res){
                    console.log(res)
                    if(res=="success"){
