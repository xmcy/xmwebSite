        $(".fixConfirmOnLine a").click(function(){
            var fixTitleName = $(".getFixTitleInput>input").val();
            var fixDescription = $("#fixDescription").val();
            var fixServiceArea = $(".myAreaPicker>input").val();
            if(fixTitleName==""){
                swal("标题不能为空！","请在标题输入框填写标题","error");
                return
            }else if(fixTitleName.length>50){
                swal("标题不能超过50个汉字或字母！","请修改您的标题","error");
                return
            }
            if(fixDescription==""){
                swal("安装描述不能为空！","详细的安装描述有助于提高接单率","error")
                return
            }else if(fixDescription.length<10||fixDescription.length>200){
                swal("请输入10到200个字的详情信息","您的标题长度有误，请修改","error")
            }
            if(fixServiceArea==""){
                swal("服务区域不能为空！","请选择您的安装服务区域","error")
                return
            }else{
                var OhomeLocation = fixServiceArea.split("/");
                if(OhomeLocation.length==1){
                    var region = {province: OhomeLocation[0],city:"", district:""}
                }else if(OhomeLocation.length==2){
                    var region={province:OhomeLocation[0],city:OhomeLocation[1],district:""}
                }else if(OhomeLocation.length==3){
                    var region={province:OhomeLocation[0],city:OhomeLocation[1],district:OhomeLocation[2]}
                }
            }
            var contactName = $(".contactNameInput>input").val();
            if(contactName==""){
                swal("联系人不能为空","","error")
            }
            var uid=$('.getThisNum').val();
            var getOrInput = $(".loadingNum001").attr("checked");
            if(getOrInput){
                var contactNum = $(".loadingNum em").html();
                var data = {
                    title:fixTitleName,
                    serviceRegion:region,
                    phoneNum:contactNum,
                    name:contactName,
                    details:fixDescription
                };
                $.ajax({
                    type:"POST",
                    url:"/resource/updateOrInsertInstall?uid="+uid,
                    data:JSON.stringify(data),
                    contentType:"application/json",
                    success:function(res){
                        if(res.result=="success"){
                            swal({
                                        title: "信息修改成功！",
                                        text: "恭喜您!您的安装信息已修改成功",
                                        type: "warning",
                                        confirmButtonColor: "#DD6B55",
                                        confirmButtonText: "确定",
                                        closeOnConfirm: false
                                    },
                                    function(isConfirm){
                                        if (isConfirm) {
                                            window.location.href="/users/myFixCenter";
                                        } else {
                                            swal("已取消", "", "error");
                                        }
                                    });
                        }
                    }
                })
            }else{
                var codeNum=$(".onInputCodeNum>input").val();
                if(codeNum == ""){
                    swal("验证码不能为空！","请输入您的验证码","error")
                    return
                }
                var contactNum = $(".onInputPhoneNum>input").val();
                var Matchok = contactNum.match(rightNum);
                if(contactNum==""){
                    swal("联系电话不能为空！","请输入您的联系电话","error");
                    return
                }else if(!Matchok){
                    swal("联系电话格式不正确！","请修改您的联系电话","error");
                    return
                }else{
                    var contactNum = $(".onInputPhoneNum>input").val();
                }
                var data = {
                    title:fixTitleName,
                    serviceRegion:region,
                    phoneNum:contactNum,
                    name:contactName,
                    details:fixDescription,
                    code:codeNum
                }
                $.ajax({
                    type:"POST",
                    url:"/resource/webUpdateOrInsertInstall?uid="+uid,
                    data:JSON.stringify(data),
                    contentType:"application/json",
                    success:function(res){
                        if(res.result=="success"){
                            swal({
                                        title: "信息修改成功！",
                                        text: "恭喜您!您的安装信息已修改成功！",
                                        type: "warning",
                                        confirmButtonColor: "#DD6B55",
                                        confirmButtonText: "确定",
                                        closeOnConfirm: false
                                    },
                                    function(isConfirm){
                                        if (isConfirm) {
                                            window.location.href="/users/myFixCenter";
                                        } else {
                                            swal("已取消", "", "error");
                                        }
                                    });
                        }
                    }
                })
            }



        })
        setTimeout(function () {
            $(".searchText").val("")
        },0)

    })
