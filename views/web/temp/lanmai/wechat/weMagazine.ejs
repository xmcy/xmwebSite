<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,height=device-height;initial-scale=1,minimum-scale=1, maximum-scale=1, user-scalable=no" />
	<title>制作宝-蓝迈传媒数字期刊</title>
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
    <link rel="stylesheet" href="/themes/<%=staticforder%>/css/weui.min.css">
    <link rel="stylesheet" href="/themes/<%=staticforder%>/css/myStyle.css">
    <link rel="stylesheet" href="/themes/<%=staticforder%>/css/common.css">
     <!--引入 WeUI -->
    <link rel="stylesheet" href="http://res.wx.qq.com/open/libs/weui/0.4.0/weui.min.css">
    <script src="/javascripts/jquery-1.8.3.min.js?vesion=<%=siteConfig.version%>"></script>
</head>
<body ontouchstart style="background: #6D6E71;">
	<div class="eMagazine">
        <div class="eMagazineTitle">
            <a href="index.html">
                <img src="/themes/<%=staticforder%>/images/web0601.png" alt="">
            </a>
        </div>
        <div class="eMagazineSlogan">
            <ul>
                <span>看广告制作杂志</span>
                <span><img src="/themes/<%=staticforder%>/images/web0602.png"></span>
                <span> 尽在制作宝</span>
            </ul>
        </div>
        <div class="eMagazineContent">
            <ul class="clearfix">
                <%if(EbookList){for(var i=0;i<EbookList.length;i++){%>
                <li class="oneMagazine">
                    <span class="perfectMagazine">
                        <span class="haveMagazine">
                        <a class="eMagazinePic" href="<%=EbookList[i].onlineView%>" style="position: relative;">
                            <img src="<%=EbookList[i].coverImg%>" alt="">
                            <!--<em class="saleNumReadNum"><span style="position: absolute;left: 1%;">发行量<%=EbookList[i].onlineConut%>万</span><span style="position: absolute;right: 1%">阅读量<%=EbookList[i].downloadConut%>万</span></em>-->
                        </a>
                        </span>
                        <span class="onLineShare clearfix">
                            <a class="eMagazineOnLine" href="<%=EbookList[i].onlineView%>">在线阅读</a>
                            <a class="eMagazineShare" href="javascript:;">分享</a>
                        </span>
                    </span>
                    <i class="eMagazineDateName" href="javascript:;"><%=EbookList[i].title.replace(/数字期刊/, "")%></i>
                </li>
                <%}}%>

            </ul>
        </div>
    </div>
    <div class="magazineCover">
        <img src="/themes/<%=staticforder%>/images/cover.png" alt="">
    </div>
    <div class="weui_tabbar weui_tabbar01">
        <a href="getHomeInfo" class="weui_tabbar_item ">
            <div class="weui_tabbar_icon">
                <img src="/themes/<%=staticforder%>/images/web0108.png" alt="">
            </div>
            <p class="weui_tabbar_label">首页</p>
        </a>
        <a href="userCenter" class="weui_tabbar_item">
            <div class="weui_tabbar_icon">
                <img src="/themes/<%=staticforder%>/images/web0109.png" alt="">
            </div>
            <p class="weui_tabbar_label">我的</p>
        </a>
    </div>
    <script src="/themes/<%=staticforder%>/js/weui.min.js"></script>
<script>
    $(function(){
        var Oheight=$(document).height();
        $('.magazineCover').css({height:Oheight});
        $('.magazineCover>img').click(function(){
            $('.magazineCover').hide();
        });
        $('.eMagazineShare').click(function(){
            $('.magazineCover').show();
        })
    })
</script>
</body>
</html>

