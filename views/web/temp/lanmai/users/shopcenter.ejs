<style>
    .shopinfoForcompanyName{
        display: inline-block;
        width: 200px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }
    .bottom_lanmai{
        margin-top: 250px;
    }
    .billStatus{
        left: 520px;
    }
    .shopcenter{
        width: 100%;
        height: 150px;
        margin: 15px 0 25px 0;
    }
    .shopcenter>div{
       float: left;
    }
    .shopcenter>.shoppic{
        width: 120px;
        height: 120px;
        margin-right: 25px;
    }
    .shopcenter>.shoppic>img{
        width: 120px;
        height: 120px;
    }
    .shopinfo,.contact{
        width:auto;
        height: 120px;
    }
    .contact{
        padding-left:28px;
    }
    .shopinfo>div,.contact>div{
        width: auto;
        height: 40px;
        line-height: 40px;
    }
    .shopinfo>div>span, .contact>div>span{
        float: left;
        width: 80px;
    }
    .shopinfo>div>ul{
        height: 40px;
        width: 200px;
        overflow: hidden;
    }
    .shopinfo>div>ul>li{
        height: 40px;
        width: auto;
        float: left;
        margin-right: 5px;
    }
    .compeletinfo{
        float: left;
        width: 90px;
        padding-left: 15px;
        background-color: #efb780;
        color: #FFFFFF;
        font-size: 14px;
        height: 20px;
        line-height: 20px;
        margin-top: 10px;
    }
    .changeIcon {
        display: inline-block;
        width: 30px;
        height: 20px;
        margin-top: 10px;
        background:#efb780 url(/themes/lanmai/images/accountSetIcon.png)  -364px -18px;
        border-left: 1px solid #e89c52;
    }
    .changeIcon>a{
        display: inline-block;
        width: 30px;
        height: 20px;
    }
    .certificate{
        height: 120px;
        width: auto;
        border-left: 1px solid gainsboro;
        padding-left: 15px;
        margin-left: 8px;
        float: right !important;
    }
    .certificate>.title{
        font-size: 15px;
        height: 25px;
        line-height: 25px;
    }
    .certificate>div>div{
        height: 60px;
        width: 60px;
        float: left;
        background:  url("/themes/lanmai/images/shopcenter.png") -73px -2px;
    }
    .certificate>div>ul{
        display: inline-block;

    }
    .certificate>div>ul>li{
        height: 20px;
        line-height: 20px;
        margin-top: 10px;
    }
    button{
        font-size: 14px;
        padding:0 3px;
        border-radius:0 ;
        height: 22px;
        line-height: 22px;
    }
    .promote{
        width: 500px;
        height: 50px;
    }
    .promote>div{
        height: 50px;
        width:55px;
        background: url("/themes/lanmai/images/shopcenter.png") -381px -10px;
        float: left;
    }
    .promote>.promoteword{
        display: inline-block;
        height: 50px;
        line-height: 70px;
        font-size: 17px;
        color: #da2f2f;
        margin-right: 10px;
    }
    .promote>.dateinfo{
        display: inline-block;
        height: 50px;
        line-height: 70px;
        font-size: 15px;
    }
    .promote>.dateinfo>span{
        margin: 0 5px;
    }
    .promotrcontent{
        width: 70%;
        height: 280px;
        display: inline-block;
        margin-top: 35px;
        font-size: 16px;
    }
    .shoppromote_contentlist{
        width: 100%;
        height: 25%;
        position: relative;
    }
    .shoppromote_contentlist>span{
        position: absolute;
        top: 0px;
        left: 0;
    }
     .shoppromote_contentlist>.step1_2{
         position: absolute;
         left:100px;
         height:40px;
         width: 480px;
         background:  url("/themes/lanmai/images/shopcenter.png") -2px -221px;
     }
     .shoppromote_contentlist>.step1_3{
         position: absolute;
         top: 0px;
         left:100px;
         height:40px;
         width: 480px;
         background:  url("/themes/lanmai/images/shopcenter.png") -2px -268px;
     }
     .shoppromote_contentlist>.step2{
         position: absolute;
         left:100px;
         height:40px;
         width: 550px;
         background:  url("/themes/lanmai/images/shopcenter.png") -2px -67px;
     }
     .shoppromote_contentlist>.step3{
         position: absolute;
         top: 0px;
         left:100px;
         height:40px;
         width: 550px;
         background:  url("/themes/lanmai/images/shopcenter.png") -2px -116px;
     }
     .shoppromote_contentlist>.step4{
         position: absolute;
         top: 0px;
         left:100px;
         height:40px;
         width: 550px;
         background:  url("/themes/lanmai/images/shopcenter.png") -2px -166px;
     }
     .attention{
        width:166px;
        padding: 0 3px;
        float: right;
         margin-right: 60px;
         margin-top: 50px;
    }
    .attention>a{
        display: block;
        width:160px;
        height: 160px;
        overflow: hidden;
    }
     .attention>a>img{
         margin: -1px 0 0 -1px;
         width:162px;
         height: 162px;
         margin-bottom:7px;
     }
     .forwemedia{
         display: none;
         border: 2px solid gainsboro;
     }
     .attention>.forwemedia>p{
         height: 15px;
         width: 160px;
         text-align: center;
         margin-bottom:5px;
         font-size: 14px;
     }
     .attention>.forwemedia>div{
         width: 160px;
         margin-top: 30px;
         position: relative;
     }
     .skin {
         width: 40px;
         height: 15px;
         position: absolute;
         bottom:6px;
         left: 40px;
     }
     .skinicon {
         width: 20px;
         height: 15px;
         float: left;
         background: url(/themes/lanmai/VRimages/vricon.png) -86px -7px;
     }
     .admakeskin>span {
         float: right;
         font-size: 12px;
     }
     .collect {
         width: 40px;
         height: 15px;
         position: absolute;
         bottom: 6px;
         left: 90px;
     }
     .collecticon {
         width: 20px;
         height: 16px;
         float: left;
         background: url(/themes/lanmai/VRimages/vricon.png) -110px -8px;
         cursor: pointer;
     }
     .attention>.forwemedia>div>span {
         float: right;
         font-size: 12px;
     }
    .identifyicon{
        padding-left: 60px;
        background-position: -124px -2px !important;
    }
    .identifystatus{
        display: inline-block;
        padding: 24px 0 0 10px;
    }
    .contacts{
        width: 155px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        display: inline-block;
    }
</style>
<div class="">
    <div class="shopcenter">
        <%if(companyInfo){%>
        <div class="shoppic"><img src="<%=companyInfo.companyLogo%>/120.120" /></div>
        <div class="shopinfo">
            <div><span>企业名称</span><a class="shopinfoForcompanyName" title="<%=companyInfo.companyName%>"><%=companyInfo.companyName%></a></div>
            <div><span>经营范围</span>
                <ul>
                    <%if(companyInfo.businessScope){for(var i=0;i<companyInfo.businessScope.length;i++){%>
                    <li><%=companyInfo.businessScope[i]%></li>
                    <%}}%>
                </ul>
            </div>
            <%if(companyInfo.region&&companyInfo.region.district){%>
            <div><span>企业地址</span><a><%=companyInfo.region.province%>-<%=companyInfo.region.city%>-<%=companyInfo.region.district%></a></div>
            <%}else if(companyInfo.region){%>
            <div><span>企业地址</span><a><%=companyInfo.region.province%>-<%=companyInfo.region.city%></a></div>
            <%}else {%>
            <div><span>企业地址</span><a></a></div>
            <%}%>
        </div>
        <div class="contact">
            <div><span>企业联系人</span><a class="contacts"><%=companyInfo.contacts%></a></div>
            <div><span>联系方式</span><a>
                    <%if(companyInfo.phoneNum){%>
                    <%=companyInfo.phoneNum%>
                    <%}else{%>
                    暂未填写
                <%}%></a></div>
            <div>
                <a  href="shopinfo"><div class="compeletinfo">完善资料</div><div class="changeIcon"><a ></a></div></a>
            </div>
        </div>
        <div class="certificate">
            <div class="title">认证情况</div>
            <%if(!companyIdentify){%>
            <div>
                <div></div>
                <ul>
                    <li>您的企业还未进行认证</li>
                    <li>资源和企业都无法优先展示！</li>
                </ul>
            </div>
            <button><a href="shopcertificate" class="a_black">点击认证</a></button>
            %>
            <%}else{%>
            <div>
                <div class="identifyicon"></div><span class="identifystatus"><%=companyIdentify.status%></span>
            </div>
            <%}%>
        </div>
        <%}else {%>
        <div class="shoppic"><img src="" /></div>
        <div class="shopinfo">
            <div><span>企业名称</span><a>暂未填写</a></div>
            <div><span>经营范围</span>
                <ul>
                    暂未填写
                </ul>
            </div>
            <div><span>企业地址</span><a>暂未填写</a></div>
        </div>
        <div class="contact" style="width: 200px">
            <div><span>企业联系人</span><a>暂未填写</a></div>
            <div><span>联系方式</span><a></a></div>
            <div>
                <a href="licenseCertification" target="_blank">
                    <div class="compeletinfo">完善资料</div>
                    <div class="changeIcon"><a></a></div>
                </a>
            </div>
        </div>
        <div class="certificate">
            <div class="title">认证情况</div>
            <div>
                <div></div>
                <ul>
                    <li>您的企业还未进行认证</li>
                    <li>资源和企业都无法优先展示！</li>
                </ul>
            </div>
            <button><a href="shopcertificate" target="_blank" class="a_black">点击认证</a></button>
        </div>
        <%}%>
    </div>

    <%if(!promotion ) {%>
    <div class="unpromotion">您的企业还未进行品牌推广，幸福天地品牌推广为您的企业准备了多种渠道，点击了解更多！<button><a href="/resource/brandWin" class="a_black">了解更多</a></button></div>
    <%}else {%>
    <div class="promote">
    <div></div>
    <span class="promoteword">品牌推广</span>
    <span class="dateinfo">
        <%if(promotion.bill.billState=="已支付"){if(promotion.period=="1季度"){%>
        <span class="dateinfo"><span><%=moment(promotion.promotionBegin).format("YYYY[年]MM[月]DD[日]")%></span> 至 <span><%=moment(promotion.promotionEnd).format("YYYY[年]MM[月]DD[日]")%></span></span>
        <%}else if(promotion.period=="半年"){%>
        <span class="dateinfo"><span><%=moment(promotion.promotionBegin).format("YYYY[年]MM[月]DD[日]")%></span> 至 <span><%=moment(promotion.promotionEnd).format("YYYY[年]MM[月]DD[日]")%></span></span>
        <%}else if(promotion.period=="1年"){%>
        <span class="dateinfo"><span><%=moment(promotion.promotionBegin).format("YYYY[年]MM[月]DD[日]")%></span> 至 <span><%=moment(promotion.promotionEnd).format("YYYY[年]MM[月]DD[日]")%></span></span>
        <%}}else {if(promotion.period=="1季度"){%>
        <span class="dateinfo"><span><%=moment(promotion.promotionBegin).format("YYYY[年]MM[月]DD[日]")%></span> 至 <span><%=moment(promotion.promotionEnd).add(3,"months").format("YYYY[年]MM[月]DD[日]")%></span></span>
        <%}else if(promotion.period=="半年"){%>
        <span class="dateinfo"><span><%=moment(promotion.promotionBegin).format("YYYY[年]MM[月]DD[日]")%></span> 至 <span><%=moment(promotion.promotionEnd).add(6,"months").format("YYYY[年]MM[月]DD[日]")%></span></span>
        <%}else if(promotion.period=="1年"){%>
        <span class="dateinfo"><span><%=moment(promotion.promotionBegin).format("YYYY[年]MM[月]DD[日]")%></span> 至 <span><%=moment(promotion.promotionEnd).add(12,"months").format("YYYY[年]MM[月]DD[日]")%></span></span>
        <%}}%>
        <%if(promotion.bill&&promotion.bill.billState=="已支付"){%>
        <!--<span class="continuicon"><a></a></span>-->
        <!--<span class="continufee"><a>续费</a></span>-->
        <%}else {%>
        <span class="billStatus">未支付</span>
        <%}%>
        </span>
    </div>
    <div>
        <div class="promotrcontent">
            <%if(promotion.priorShowState=="沟通传递素材" ) {%>
            <div class="shoppromote_contentlist">
                <span>优先展示</span>
                <div class="step1_2"></div>
            </div>
            <%}else{%>
            <div class="shoppromote_contentlist priorShow">
                <span>优先展示</span><input hidden value="<%=promotion.priorShowImgs%>"/>
                <div class="step1_3"></div>
            </div>
            <%}%>

            <%if(promotion.eBookState=="未购买" ) {%>
            <%}else if(promotion.eBookState=="沟通传递素材"){%>
            <div class="shoppromote_contentlist ">
                <span>数字期刊</span>
                <div class="step2"></div>
            </div><%}else if(promotion.eBookState=="制作中"){%>
            <div class="shoppromote_contentlist">
                <span>数字期刊</span>
                <div class="step3"></div>
            </div>
            <%}else{%>
            <div class="shoppromote_contentlist eBooks">
                <span>数字期刊</span><input hidden value="<%=promotion.ebookImgs%>"/>
                <div class="step4"></div>
            </div>
            <%}%>

            <%if(promotion.VRState=="未购买" ) {%>
            <%}else if(promotion.VRState=="沟通传递素材"){%>
            <div class="shoppromote_contentlist ">
                <span>VR展馆</span>
                <div class="step2"></div>
            </div><%}else if(promotion.VRState=="制作中"){%>
            <div class="shoppromote_contentlist">
                <span>VR展馆</span>
                <div class="step3"></div>
            </div>
            <%}else{%>
            <div class="shoppromote_contentlist VR">
                <span>VR展馆</span><input hidden value="<%=promotion.vrImgs%>"/>
                <div class="step4"></div>
            </div>
            <%}%>

            <%if(promotion.weMediaState=="未购买" ) {%>
            <%}else if(promotion.weMediaState=="沟通传递素材"){%>
            <div class="shoppromote_contentlist">
                <span>自媒体渠道</span>
                <div class="step2"></div>
            </div><%}else if(promotion.weMediaState=="制作中"){%>
            <div class="shoppromote_contentlist">
                <span>自媒体渠道</span>
                <div class="step3"></div>
            </div>
            <%}else{%>
            <div class="shoppromote_contentlist weMedia">
                <span>自媒体渠道</span><input hidden value="<%=promotion.weMediaImgs%>"/>
                <div class="step4"></div>
            </div>
            <%}%>
        </div>

        <div class="attention">
            <a href="../resource/vr"> <img /></a>
            <div class="forwemedia">
                <p>扫码手机查阅</p>
                <p>幸福天地公众号宣传文</p>
                <div>
                    <div class="skin"><span class="skinicon"></span><span>66</span></div>
                    <div class="collect"><span class="collecticon"></span><span>66</span></div>
                </div>
            </div>

        </div>

    </div>
    <%}%>

</div>
<script>
    $(function () {
        if($(".weMedia")){
            $(".weMedia").on("mouseenter",function () {
                var imgsrc=$(this).children("input").val()
                $(".attention img").attr("src",imgsrc);
                $(".attention a").attr("href","");
                $(".forwemedia").show()
            });

        }

        if($(".VR")){
            $(".VR").on("mouseenter",function () {
                var imgsrc=$(this).children("input").val()
                $(".attention img").attr("src",imgsrc);
                $(".attention a").attr("href","../resource/vr");
                $(".forwemedia").hide()
            });
        }

        if($(".eBooks")){
            $(".eBooks").on("mouseenter",function () {
                var imgsrc=$(this).children("input").val()
                $(".attention img").attr("src",imgsrc);
                $(".attention a").attr("href","../resource/digitalMagazine");
                $(".forwemedia").hide()
            });
        }

        if($(".priorShow")){
            $(".priorShow").on("mouseenter",function () {
                var imgsrc=$(this).children("input").val()
                $(".attention img").attr("src",imgsrc);
                $(".attention a").attr("href","../resource/brandWin");
                $(".forwemedia").hide()
            });
        }

    })
</script>